{
  "name": "landalousie",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "scripts": {
    "start": "run-p -l type:check:watch start:dev start:stripe-cli",
    "start:dev": "vite dev --host --port 8080",
    "start:stripe-cli": "dotenvx run --env-file=.env.local -- docker run --rm -e STRIPE_API_KEY --entrypoint /bin/sh stripe/stripe-cli:v1.35.0 -c \"stripe listen --api-key $STRIPE_API_KEY --forward-to http://host.docker.internal:8080/api/checkout/events\"",
    "start:email": "email dev --dir ./src/email/templates",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "type:check": "tsc --noEmit",
    "type:check:watch": "tsc --noEmit --watch",
    "prepare": "husky || echo 'Husky is not installed'"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@content-island/api-client": "^0.16.0",
    "@fontsource/noto-color-emoji": "^5.2.11",
    "@nanostores/persistent": "^1.3.3",
    "@nanostores/react": "^1.0.0",
    "@react-email/components": "^1.0.7",
    "@tanstack/react-devtools": "^0.9.5",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.159.10",
    "@tanstack/react-router-devtools": "^1.159.10",
    "@tanstack/react-router-ssr-query": "^1.159.10",
    "@tanstack/react-start": "^1.159.12",
    "clsx": "^2.1.1",
    "marked": "^17.0.2",
    "nanostores": "^1.1.0",
    "nitro": "npm:nitro-nightly@latest",
    "pino": "^10.3.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.2",
    "rollbar": "^2.26.5",
    "stripe": "^20.3.1",
    "vest": "^5.4.6",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@react-email/preview-server": "^5.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.5.1",
    "@tanstack/router-plugin": "^1.159.12",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "react-email": "^5.2.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "web-vitals": "^5.1.0"
  }
}
